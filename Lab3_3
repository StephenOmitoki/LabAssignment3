#include <iostream>
using namespace std;


int main() {

	typedef int* VEC;
	typedef VEC* MATRIX;

	int a = 5;
	int* ptr_a = &a;
	VEC v = &a;
	VEC* ptr_v = &v ;
	MATRIX m = &v;

	cout << "Value of a using v: " << *v << endl;
	cout << "Value of a using m: " << **m << endl;

	return 0;
}

